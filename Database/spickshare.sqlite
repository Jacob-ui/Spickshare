CREATE TABLE user (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    first_name  TEXT NOT NULL,
    last_name TEXT NOT NULL,
    email TEXT NOT NULL
);
CREATE TABLE spicker (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    label TEXT NOT NULL,
    cost INTEGER
);
CREATE TABLE user_spicker (
    user_id INTEGER,
    spicker_id INTEGER,
    PRIMARY KEY (user_id, spicker_id),
    FOREIGN KEY (user_id) REFERENCES user (id) ON DELETE CASCADE,
    FOREIGN KEY (spicker_id) REFERENCES spicker (id) ON DELETE CASCADE
);